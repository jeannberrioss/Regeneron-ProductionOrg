    <formula>IF ( TOTAL_Collaboration_Budget_M__c &gt; TOTAL_Collaboration_Budget_S__c , TOTAL_Collaboration_Budget_M__c, TOTAL_Collaboration_Budget_S__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Contracted Budget</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
