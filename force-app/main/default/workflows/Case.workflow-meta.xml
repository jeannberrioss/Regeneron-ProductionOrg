<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <rules>
        <fullName>AssignedTo Field Update</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Case.CreatedById</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Updating Created by name into the Assigned To Field in Case</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
